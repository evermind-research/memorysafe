import numpy as np
import matplotlib.pyplot as plt

print("MemorySafe Public Demo Loaded")

# Fake demo just to validate pipeline
steps = np.arange(0, 100)
fifo = np.exp(-steps/50)
memorysafe = np.exp(-steps/200)

plt.plot(steps, fifo, label="FIFO")
plt.plot(steps, memorysafe, label="MemorySafe")
plt.xlabel("Training steps")
plt.ylabel("Critical recall")
plt.legend()
plt.title("MemorySafe vs FIFO (Toy Demo)")
plt.show()
